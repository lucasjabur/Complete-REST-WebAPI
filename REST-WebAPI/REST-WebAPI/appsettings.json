{
  "MSSQLServerSQLConnection": {
    "MSSQLServerSQLConnectionString": "Data Source=localhost\\SQLEXPRESS;Initial Catalog=ASP.NET10;Integrated Security=True;TrustServerCertificate=true;"
  },
  "Cors": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:8080",
      "http://localhost:5063",
      "https://localhost:7161",
      "https://erudio.com.br"
    ]
  },
  "TokenConfiguration": {
    "Audience": "ExempleAudience",
    "Issuer": "ExempleIssuer",
    "Secret": "THIS_IS_ERUDIO_SUPER_SUPER_SECRET_KEY",
    "Minutes": 60,
    "DaysToExpiry": 7
  },
  "Email": {
    "Subject": "Default Subject",
    "Message": "This is a default message from the API",
    "Host": "smtp.gmail.com",
    "Port": 587,
    "Username": "${EMAIL_USERNAME}",
    "Password": "${EMAIL_PASSWORD}",
    "Proporties": {
      "SmtpAuth": "true",
      "StartTlsEnable": "true",
      "StartTlsRequired": "true"
    },
    "Ssl": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "REST_WebAPI": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*"
}
